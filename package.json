{
  "name": "redux-views",
  "version": "0.7.1",
  "license": "MIT",
  "repository": "github:josepot/redux-views",
  "bugs": "https://github.com/josepot/redux-views/issues",
  "keywords": [
    "redux",
    "selectors",
    "cached",
    "functional"
  ],
  "author": "Josep M Sobrepere (https://github.com/josepot)",
  "main": "lib/redux-views.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/redux-views.js",
  "module": "es/redux-views.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types/index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test",
    "pretest": "npm run build",
    "test": "jest \"(/test/.*\\.spec.js)$\"",
    "test-transforms": "jest \"(/transforms/__tests__/.*.js)$\"",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "test:types": "dtslint types",
    "build": "cross-env NODE_ENV=production rollup -c",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test"
  },
  "dependencies": {
    "loose-envify": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.0",
    "dtslint": "^0.9.8",
    "eslint": "^6.4.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.1.1",
    "glob": "^7.1.3",
    "jest": "^24.9.0",
    "jscodeshift": "^0.6.4",
    "prettier": "^1.16.1",
    "ramda": "^0.26.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "npmName": "redux-views",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "testURL": "http://localhost/"
  },
  "sideEffects": false
}
